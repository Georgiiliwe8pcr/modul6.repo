import json

class PhoneBook:
    def __init__(self):
        self.phone_book = {}

    def add_phone(self, name, phone):
        if name in self.phone_book:
            self.phone_book[name].append(phone)
        else:
            self.phone_book[name] = [phone]

    def remove_phone(self, name, phone):
        # Implement the method to remove a phone number
        if name in self.phone_book:
            try:
                self.phone_book[name].remove(phone)
                if not self.phone_book[name]:  # If the list is empty, remove the name
                    del self.phone_book[name]
            except ValueError:
                print(f"Phone number {phone} not found for {name}.")
        else:
            print(f"Name {name} not found in phone book.")

    def show_phone_book(self):
        return json.dumps(self.phone_book, indent=4)

if __name__ == "__main__":
    pb = PhoneBook()
    pb.add_phone("John", "12345")
    pb.add_phone("John", "67890")
    pb.add_phone("Jane", "54321")

    print("Phone book before removal:")
    print(pb.show_phone_book())

    pb.remove_phone("John", "12345")
    print("\nPhone book after removing John's 12345:")
    print(pb.show_phone_book())

    pb.remove_phone("John", "11111")  # Nonexistent phone number
    pb.remove_phone("Jake", "54321")  # Nonexistent name
